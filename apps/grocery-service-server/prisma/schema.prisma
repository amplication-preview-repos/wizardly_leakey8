datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  brand       String?
  createdAt   DateTime @default(now())
  id          String   @id @default(cuid())
  productName String?
  upc12Number Int?
  updatedAt   DateTime @updatedAt
}

model SearchHistory {
  brand       String?
  createdAt   DateTime @default(now())
  id          String   @id @default(cuid())
  productName String?
  updatedAt   DateTime @updatedAt
}
